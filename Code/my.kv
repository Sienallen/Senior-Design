                pos_hint: {"x": 0.45, "y": 0.85}
                font_size: 50
            Label:
                id: authors
                text: "A project by Ian, Allen and Devin"
                color: (1, 1, 1, 1)
                background_color: (0, 0, 0, 1)
                size_hint: (0.1, 0.1)
                pos_hint: {"x": 0.45, "y": 0.7}
                font_size: 24
            Button:
                id: gameButton
                text: "Play Game"
                color: (1, 1, 1, 1)
                background_color: (0, 0, 0, 1)
