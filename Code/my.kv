                    root.manager.current = "GameModesScreen"
                    root.manager.transition.direction = "left"
            Button:
                id: settingsButton
                text: "Settings"
                color: (1, 1, 1, 1)
                background_color: (0, 0, 0, 1)
                size_hint: (0.2, 0.1)
                pos_hint: {"x": 0.4, "y": 0.35}
                font_size: 12
                on_press:
                    root.manager.current = "SettingsScreen"
                    root.manager.transition.direction = "left"
            Button:
                id: exitButton
                text: "Exit"
                color: (1, 1, 1, 1)
                background_color: (0, 0, 0, 1)
                size_hint: (0.1, 0.1)
                pos_hint: {"x": 0.45, "y": 0.05}
                font_size: 12
                on_press:
                    root.ClosingClient()
                    app.get_running_app().stop()



