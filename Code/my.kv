<GameModesScreen>:
    appVsApp: appVsApp
    boardVsAppAsApp: boardVsAppAsApp
    boardVsAppAsBoard: boardVsAppAsBoard
    appVsAi: appVsAi
    boardVsAi: boardVsAi
    gamemodeLayout: gamemodeLayout
    gameModeTitle: gameModeTitle
    backButton: backButton

# ---------------------------------------------------------------------------------------------------------------

    FloatLayout:
        RelativeLayout:
            id: gamemodeLayout
            size_hint: (min(root.width, root.height) / root.width, min(root.width, root.height) / root.height)
            pos_hint: {"x": 0.5 - min(root.width, root.height) / root.width / 2, "y": 0.5 - min(root.width, root.height) / root.height / 2}
            
            Label:
                id: gameModeTitle
                text: "Game Mode Selection"
                color: (1, 1, 1, 1)
                background_color: (0, 0, 0, 1)
                size_hint: (0.1, 0.1)
                pos_hint: {"x": 0.45, "y": 0.85}
                font_size: 36
            Button:
                id: appVsApp
                text: "App vs. App"
                color: (1, 1, 1, 1)
                background_color: (0, 0, 0, 1)
                size_hint: (0.2, 0.1)
                pos_hint: {"x": 0.4, "y": 0.55}
                font_size: 12
                on_release: 
                    root.GamemodeSelection(self)
                    root.manager.current = "ColorScreen"
                    root.manager.transition.direction = "left"
            Button:
                id: boardVsAppAsApp
                text: "Board vs. App (App)"
                color: (1, 1, 1, 1)
                background_color: (0, 0, 0, 1)
                size_hint: (0.2, 0.1)
                pos_hint: {"x": 0.27, "y": 0.4}
                font_size: 12
                on_release: 
                    root.GamemodeSelection(self)
                    root.manager.current = "ColorScreen"
                    root.manager.transition.direction = "left"
            Button:
                id: boardVsAppAsBoard
                text: "Board vs. App (Board)"
                color: (1, 1, 1, 1)
                background_color: (0, 0, 0, 1)
                size_hint: (0.2, 0.1)
                pos_hint: {"x": 0.53, "y": 0.4}
                font_size: 12
                on_release: 
                    root.GamemodeSelection(self)
                    root.manager.current = "ColorScreen"
                    root.manager.transition.direction = "left"
            Button:
                id: appVsAi
                text: "App vs. AI"
                color: (1, 1, 1, 1)
                background_color: (0, 0, 0, 1)
                size_hint: (0.2, 0.1)
                pos_hint: {"x": 0.27, "y": 0.25}
                font_size: 12
                on_release:
                    root.GamemodeSelection(self)
                    root.manager.current = "ColorScreen"
                    root.manager.transition.direction = "left"
            Button:
                id: boardVsAi
                text: "Board vs. AI"
                color: (1, 1, 1, 1)
                background_color: (0, 0, 0, 1)
                size_hint: (0.2, 0.1)
                pos_hint: {"x": 0.53, "y": 0.25}
                font_size: 12
                on_release:
                    root.GamemodeSelection(self)
                    root.manager.current = "ColorScreen"
                    root.manager.transition.direction = "left"
            Button:
