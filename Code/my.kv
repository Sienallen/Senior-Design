<LoginScreen>:
    login: login
    username: username
    password: password
    loginLayout: loginLayout
    title: title
    usernameLabel: usernameLabel
    passwordLabel: passwordLabel
    loginButton: loginButton
    signUpButton: signUpButton
    exitButton: exitButton
    
# ---------------------------------------------------------------------------------------------------------------

    FloatLayout:
        RelativeLayout:
            id: loginLayout
            size_hint: (min(root.width, root.height) / root.width, min(root.width, root.height) / root.height)
            pos_hint: {"x": 0.5 - min(root.width, root.height) / root.width / 2, "y": 0.5 - min(root.width, root.height) / root.height / 2}
            
            Label:
                id: title
                text: "Wizard Chess"
                color: (1, 1, 1, 1)
                background_color: (0, 0, 0, 1)
                size_hint: (0.1, 0.1)
                pos_hint: {"x": 0.45, "y": 0.85}
                font_size: 50
            Label:
                id: login
                text: ""
                color: (1, 1, 1, 1)
                background_color: (0, 0, 0, 1)
                size_hint: (0.1, 0.1)
                pos_hint: {"x": 0.45, "y": 0.75}
                font_size: 36
            Label:
                id: usernameLabel
                text: "Please Enter A Username"
                color: (1, 1, 1, 1)
                background_color: (0, 0, 0, 1)
                size_hint: (0.1, 0.1)
                pos_hint: {"x": 0.45, "y": 0.65}
                font_size: 24
            Label:
                id: passwordLabel
                text: "Please Enter A Password"
                color: (1, 1, 1, 1)
                background_color: (0, 0, 0, 1)
                size_hint: (0.1, 0.1)
                pos_hint: {"x": 0.45, "y": 0.45}
                font_size: 24
            TextInput:
                id: username
                text: ""
                color: (1, 1, 1, 1)
                background_color: (1, 1, 1, 1)
                size_hint: (0.4, 0.1)
                pos_hint: {"x": 0.3, "y": 0.55}
                font_size: 16
                hint_text: "Username"
                hint_text_color: (0.5, 0.5, 0.5, 1.0)
                multiline: False
                cursor_blink: True
                cursor_color: (0, 0, 0, 1)
            TextInput:
                id: password
                text: ""
                color: (1, 1, 1, 1)
                background_color: (1, 1, 1, 1)
                size_hint: (0.4, 0.1)
                pos_hint: {"x": 0.3, "y": 0.35}
                font_size: 16
                hint_text: "Password"
                hint_text_color: (0.5, 0.5, 0.5, 1.0)
                multiline: False
                cursor_blink: True
                cursor_color: (0, 0, 0, 1)
            Button:
                id: loginButton
                text: "Log in"
                color: (1, 1, 1, 1)
                background_color: (0, 0, 0, 1)
                size_hint: (0.2, 0.1)
                pos_hint: {"x": 0.27, "y": 0.2}
                font_size: 12
                on_press:
                    root.LogIn()
            Button:
                id: signUpButton
                text: "Sign Up"
                color: (1, 1, 1, 1)
                background_color: (0, 0, 0, 1)
                size_hint: (0.2, 0.1)
                pos_hint: {"x": 0.53, "y": 0.2}
                font_size: 12
                on_press:
                    root.SignUp()
            Button:
                id: exitButton
                text: "Exit"
                color: (1, 1, 1, 1)
                background_color: (0, 0, 0, 1)
                size_hint: (0.1, 0.1)
                pos_hint: {"x": 0.45, "y": 0.05}
                font_size: 12
                on_press:
                    root.LogClosingClient()
                    app.get_running_app().stop()



